  "main": "dist/roman-dates.js",
  "types": "dist/roman-dates.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "tsc": "tsc",
    "build": "WEBPACK_ENV=build webpack && npm run test",
