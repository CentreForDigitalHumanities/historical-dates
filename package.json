  },
  "homepage": "https://github.com/UUDigitalHumanitieslab/roman-dates#readme",
  "devDependencies": {
    "@types/es6-shim": "^0.31.35",
    "@types/jasmine": "^2.6.2",
    "@types/karma": "^1.7.1",
    "jasmine": "^2.8.0",
    "karma": "^2.0.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-typescript": "^3.0.8",
    "phantomjs-prebuilt": "^2.1.7",
    "ts-loader": "^3.1.1",
    "webpack": "^3.8.1"
